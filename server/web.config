<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="index.js" verb="*" modules="iisnode" resourceType="File" />
        </handlers>

        <rewrite>
            <rules>
                <rule name="ReqToIndexjs">
                    <match url=".*" />
                    <action type="Rewrite" url="index.js" />
                </rule>
            </rules>
        </rewrite>

    </system.webServer>
</configuration>